{
  "name": "mysiar/clean-php-data-reader",
  "type": "project",
  "license": "GPL-3",
  "authors": [
    {
      "name": "Piotr Synowiec",
      "email": "psynowiec@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-simplexml": "*",
    "ext-yaml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symplify/easy-coding-standard": "^10.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
